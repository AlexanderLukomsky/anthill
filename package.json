{
  "name": "inctagram",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*",
    "packages.icons/*",
    "configs/*"
  ],
  "scripts": {
    "build": "turbo run build --no-daemon",
    "dev": "turbo run dev --parallel --no-daemon",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "format-check": "prettier --check \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint": "8.42.0",
    "eslint-config-custom": "*",
    "prettier": "^2.8.7",
    "turbo": "^1.9.7"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
